{
	"name": "librgrn",
	"version": "1.0.0",
	"description": "Pure Node.js cryptographic random & security utilities",
	"keywords": [
		"random",
		"cryptography",
		"security",
		"jwt",
		"crypto",
		"token"
	],
	"homepage": "https://github.com/neuxdotdev/libgrnd#readme",
	"bugs": {
		"url": "https://github.com/neuxdotdev/libgrnd/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neuxdotdev/libgrnd.git"
	},
	"license": "AGPL-3.0",
	"author": "neuxdotdev",
	"type": "module",
	"main": "./dist/lib.js",
	"types": "./dist/types.d.ts",
	"exports": {
		".": {
			"types": "./dist/types.d.ts",
			"import": "./dist/lib.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"build:ts": "tsc",
		"build:types": "dts-bundle-generator -o dist/types.d.ts dist/lib.d.ts",
		"build": "npm run clean && npm run build:ts && npm run build:types",
		"dev": "tsc --watch",
		"fmt": "prettier --write .",
		"lint": "tsc --noEmit",
		"test": "node dist/lib.js",
		"prepare": "husky install",
		"format": "prettier . --write",
		"format:check": "prettier . --check",
		"clean-code": "rmcm -l js -i -c 0 -r src --verbose && npm run fmt"
	},
	"dependencies": {
		"jsonwebtoken": "^9.0.3"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^25.3.0",
		"dts-bundle-generator": "^9.5.1",
		"esbuild": "^0.27.3",
		"husky": "^9.1.7",
		"prettier": "^3.8.1",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18"
	}
}
